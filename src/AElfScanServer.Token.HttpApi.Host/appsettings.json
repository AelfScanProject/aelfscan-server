{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Debug",
        "System": "Warning",
        "Microsoft": "Warning",
        "Orleans": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": { "type": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact" }
        }
      },
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "formatter": { "type": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact" },
          "rollOnFileSizeLimit": true,
          "rollingInterval": "Day",
          "retainedFileCountLimit": 15
        }
      }
    ]
  },
  "App": {
    "CorsOrigins": "https://*.SignatureServer.com,http://localhost:4200"
  },
  "AuthServer": {
